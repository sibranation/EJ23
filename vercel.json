{
	"version": 2,
	"builds": [
		{ "src": "radio-station-web/package.json", "use": "@vercel/static-build", "config": { "distDir": "radio-station-web/dist/radio-station-web/browser" } },
		{ "src": "api/ws.js", "use": "@vercel/edge" }
	],
	"routes": [
		{ "src": "/api/ws", "dest": "/api/ws.js" },
		{ "src": "/assets/(.*)", "headers": { "cache-control": "public, max-age=31536000, immutable" }, "dest": "/radio-station-web/dist/radio-station-web/browser/assets/$1" },
		{ "src": "/(.*)\\.(css|js|png|jpg|jpeg|gif|svg|ico)", "headers": { "cache-control": "public, max-age=31536000, immutable" }, "dest": "/radio-station-web/dist/radio-station-web/browser/$1.$2" },
		{ "src": "/(.*)", "dest": "/radio-station-web/dist/radio-station-web/browser/index.html" }
	],
	"functions": {
		"api/ws.js": {
			"runtime": "@vercel/edge"
		}
	}
}

